{
    "apps": [
        {
            "name": "rw-express-seed",
            "script": "./bin/www/server.js",
            "watch": true,
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "ignore_watch": [
                "node_modules",
                "data"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "env_production": {
                "NODE_ENV": "production"
            },
            "env_staging": {
                "NODE_ENV": "staging"
            }
        }
    ]
}